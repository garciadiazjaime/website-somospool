    "babel-preset-react": "^6.3.13",
    "bower": "^1.7.2",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "gulp": "^3.9.0",
    "gulp-clean-css": "^2.0.4",
    "gulp-concat": "^2.6.0",
    "gulp-copy": "0.0.2",
    "gulp-if": "^2.0.0",
    "gulp-minify": "0.0.7",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^1.5.3",
    "install": "^0.6.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "npm": "^3.8.3",
    "prettydiff": "^1.16.37",
    "react-hot-loader": "^1.3.0",
    "run-sequence": "^1.1.5",
    "sass-loader": "^3.1.2",
    "sprity": "^1.0.8",
    "sprity-js": "^1.1.1",
    "sprity-sass": "^1.0.4",
    "style-loader": "^0.13.0",
