{
  "name": "website-somospool",
  "version": "0.0.1",
  "description": "somospool website",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "clean": "rm -rf lib",
    "babel": "./node_modules/.bin/babel src --out-dir lib -D",
    "dev-server": "node lib/server/webpack",
    "server": "./node_modules/.bin/nodemon lib/server/server",
    "dev": "npm run babel & npm run dev-server & npm run server",
    "start": "node lib/server/server.js",
    "build": "npm run clean && npm run babel",
    "sprites": "./node_modules/.bin/gulp build:sprites",
    "lint": "./node_modules/.bin/eslint --ignore-pattern **/app-*.js, **/**/sprite.js --ext .js,.jsx src",
    "deploy": "NODE_ENV=production webpack -p --config webpack.config.prod.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.15.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "body-parser": "^1.14.2",
    "convict": "^1.0.2",
    "express": "^4.13.3",
    "history": "^1.17.0",
    "jade": "^1.11.0",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.3",
    "rest": "^1.3.1",
    "sendgrid": "^2.0.0",
    "webpack-sources": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/garciadiazjaime/website-boilerplate"
  }
}
