    "convict": "^1.0.2",
    "cryptiles": "^3.0.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "history": "^1.17.0",
    "hoek": "^3.0.4",
    "is-promise": "^2.1.0",
    "jade": "^1.11.0",
    "jquery": "^2.2.1",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.3",
    "react-tap-event-plugin": "^0.2.2",
    "readable-stream": "^2.0.5",
    "rest": "^1.3.1",
    "sendgrid": "^2.0.0",
    "sntp": "^2.0.0",
